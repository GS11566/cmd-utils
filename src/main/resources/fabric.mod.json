	"authors": [
		"Coderx_Gamer",
		"MrBreakNFix"
	],
	"contact": {
		"homepage": "https://github.com/Coderx-Gamer/cmd-utils/",
		"sources": "https://github.com/Coderx-Gamer/cmd-utils/"
